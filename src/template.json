  "tokenColors": [
    {
      "name": "Source default",
      "scope": "source",
      "settings": {
        "foreground": "{{ palette.lighterGray }}"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "{{ palette.darkGray }}",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "{{ palette.blue }}"
      }
    },
    {
      "name": "Storage keywords",
      "scope": "storage",
      "settings": {
        "foreground": "{{ palette.blue }}"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "{{ palette.green }}"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.default",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.import"
      ],
      "settings": {
        "foreground": "{{ palette.royalBlue }}"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.type.class",
        "support.class.builtin"
      ],
      "settings": {
        "foreground": "{{ palette.richYellow }}{{ alpha.high }}"
      }
    },
    {
      "name": "Typescript primitive types",
      "scope": [
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "{{ palette.lightYellow }}"
      }
    },
    {
      "name": "Typescript advanced types",
      "scope": [
        "entity.name.type",
        "entity.name.tag",
        "entity.name.type.alias",
        "entity.name.type.interface",
        "meta.type.annotation",
        "meta.type.declaration"
      ],
      "settings": {
        "foreground": "{{ palette.lightYellow }}"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "{{ palette.lightBlue }}"
      }
    },
    {
      "name": "Entity name tag",
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "{{ palette.yellow }}"
      }
    },
    {
      "name": "Component prop",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "{{ palette.lightestGreen2 }}",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "keyword.operator.assignment",
        "keyword.operator.spread",
        "keyword.operator.type.annotation",
        "meta.brace.round",
        "meta.brace.square",
        "meta.indexer.declaration"
      ],
      "settings": {
        "foreground": "{{ palette.lighterPurple }}"
      }
    },
    {
      "name": "Module",
      "scope": "entity.name.type.module",
      "settings": {
        "foreground": "{{ palette.green }}"
      }
    },
    {
      "name": "Boolean, null, undefined, number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "{{ palette.orange }}"
      }
    },
    {
      "name": "This",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "{{ palette.lightPurple }}",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Object property",
      "scope": [
        "meta.parameter.object-binding-pattern",
        "variable.object.property",
        "variable.other.object.property",
        "meta.template.expression",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "{{ palette.lighterGray }}"
      }
    },
    {
      "name": "Object key",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "{{ palette.lightestBlue }}"
      }
    },
    {
      "name": "Quotation marks",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "{{ palette.green }}"
      }
    }
  ]
}